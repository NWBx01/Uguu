{
  "dest": "dist",
  "pkgVersion": "1.8.6",
  "pages": [
    "index.ejs",
    "faq.ejs",
    "api.ejs"
  ],
  "DEBUG": ${DEBUG},
  "BENCHMARK_MODE": ${BENCHMARK_MODE},
  "max_upload_size": ${MAX_UPLOAD_SIZE},
  "expireTime": ${EXPIRE_TIME},
  "expireTimeUnit": "${EXPIRE_TIME_UNIT}",
  "siteName": "${SITE_NAME}",
  "subTitle": "${SITE_SUBTITLE}",
  "DOMAIN": "${DOMAIN}",
  "FILE_DOMAIN": "${FILE_DOMAIN}",
  "abuseContact": "${ABUSE_CONTACT}",
  "infoContact": "${INFO_CONTACT}",
  "ServerCountryLocation": "${SERVER_CN_LOC}",
  "SiteMetaInfo": "${SITE_META_INFO}",
  "responseTime": "${RESPONSE_TIME}",
  "showGithub": ${SHOW_GITHUB},
  "donationBanner": ${SHOW_DONATION_BANNER},
  "paypalUrl": "${PAYPAL_URL}",
  "bitcoinAddress": "${BTC_ADDR}",
  "ethereumAddress": "${ETH_ADDR}",
  "kofiUrl": "${KOFI_URL}",
  "malwareBanner": ${SHOW_MALWARE_BANNER},
  "DB_MODE": "${DB_MODE}",
  "DB_PATH": "${DB_PATH}",
  "DB_USER": "${DB_USER}",
  "DB_PASS": "${DB_PASS}",
  "LOG_IP": ${LOG_IP},
  "ANTI_DUPE": ${ANTI_DUPE},
  "BLACKLIST_DB": ${BLACKLIST_DB},
  "FILTER_MODE":${FILTER_MODE},
  "RATE_LIMIT": ${RATE_LIMIT},
  "RATE_LIMIT_TIMEOUT": ${RATE_LIMIT_TIMEOUT},
  "RATE_LIMIT_FILES": ${RATE_LIMIT_FILES},
  "FILES_ROOT": "${FILES_ROOT}",
  "FILES_RETRIES": ${FILES_RETRIES},
  "NAME_LENGTH": ${NAME_LENGTH},
  "ID_CHARSET": "${ID_CHARSET}",
  "DOUBLE_DOTS_EXTENSIONS": [
    "tar.bz2",
    "tar.gz",
    "min.css",
    "min.js",
    "conf.bak"
  ],
  "FILTER_EXTENSIONS": [
    "exe",
    "scr",
    "com",
    "vbs",
    "bat",
    "cmd",
    "htm",
    "html",
    "jar",
    "msi",
    "apk",
    "phtml",
    "svg"
  ],
  "FILTER_MIME": [
    "application/msword",
    "text/html",
    "application/x-dosexec",
    "application/java",
    "application/java-archive",
    "application/x-executable",
    "application/x-mach-binary",
    "image/svg+xml"
  ]
}
